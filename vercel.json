{
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": ".",
  "devCommand": "python3 -m http.server 3000",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
